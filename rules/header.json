{
    "settings": [],
    "navigation": [],
    "rules": {
        "logo": {
            "label": "Logo",
            "type": "singlefile",
            "show": "yes",
            "default": "logo.png",
            "system_val": "{{site.logo}}"
        },
        "title": {
            "label": "Site Title",
            "type": "quickEditor",
            "show": "yes",
            "default": "Couchly Store",
            "system_val": "{{site.title}}"
        },
        "announcement": {
            "label": "Announcement",
            "type": "quickEditor",
            "show": "yes",
            "default": "Get 35% off for Hot Deal"
        },
        "phone": {
            "label": "Contact Number",
            "type": "number",
            "show": "yes",
            "default": "+11-789-2323",
            "system_val": "{{site.phone}}"
        },
        "email": {
            "label": "Mail",
            "type": "email",
            "show": "yes",
            "default": "contact@domain-name.com",
            "system_val": "{{site.email}}"
        },
        "social": {
            "label": "Social",
            "type": "socialicon",
            "show": "no"
        }
    }
}