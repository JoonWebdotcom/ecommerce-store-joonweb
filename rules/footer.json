{
    "settings": [],
    "rules": {
        "title": {
            "label": "Site Title",
            "type": "quickEditor",
            "show": "yes",
            "default": "{{ site.title }}"
        },
        "subtitle": {
            "label": "Site Tagline",
            "type": "quickEditor",
            "show": "yes",
            "default": "Lorem ipsum, dolor sit amet consectetur adipisicing elit. Cupiditate, saepe."
        },
        "logo": {
            "label": "Logo",
            "type": "singlefile",
            "show": "yes",
            "default": "logo.png"
        },
        "phone": {
            "label": "Contact Number",
            "type": "number",
            "show": "yes",
            "default": "+11-789-2323",
            "system_val": "{{site.phone}}"
        },
        "email": {
            "label": "Mail",
            "type": "email",
            "show": "yes",
            "default": "contact@domain-name.com",
            "system_val": "{{site.email}}"
        },
        "address": {
            "label": "Address",
            "type": "text",
            "show": "yes",
            "default": "212th Street Model Town, NY, 123098",
            "system_val": "{{site.address}}"
        },
        "social": {
            "label": "Social",
            "type": "socialicon",
            "show": "no"
        },
        "form": {
            "label": "Subscribe Form",
            "type": "siteform",
            "show": "yes",
            "default": "subscribe"
        },
        "copyright": {
            "label": "Copyright Text",
            "type": "text",
            "show": "yes",
            "default": "Copyright \u00a9 {{ site.title }} {{ date.year }}. All rights reserved."
        },
        "powered": {
          "label": "Powered By",
          "type": "text",
          "show": "yes",
          "default": "Powered by Joonweb.com"
        }
    }
  }